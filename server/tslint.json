{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "no-console": false,
    "prefer-const": false,
    "semicolon": [
      true,
      "never"
    ],
    "space-before-function-paren": [
      true,
      {
        "anonymous": "always",
        "asyncArrow": "always",
        "constructor": "always",
        "method": "always",
        "named": "always"
      }
    ],
    "quotemark": [
      true,
      "single"
    ],
    "interface-name": [
      false
    ],
    "member-access": [
      true,
      "no-public"
    ],
    "member-ordering": [
      true,
      {
        "order": [
          "static-field",
          "instance-field",
          "static-method",
          "instance-method"
        ]
      }
    ],
    "variable-name": [
      true,
      "allow-leading-underscore"
    ]
  },
  "rulesDirectory": []
}
